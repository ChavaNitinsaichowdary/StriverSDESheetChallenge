import java.util.* ;
import java.io.*; 
import java.util.ArrayList;

public class Solution{
    public static int maximumProfit(ArrayList<Integer> prices){
        // Write your code here.
        int lsf = Integer.MAX_VALUE;
        int ans = 0;
        int curr = 0;
        for(int i = 0;i<prices.size();i++){
            if(prices.get(i)<lsf){
                lsf = prices.get(i);
            }
            curr = prices.get(i)-lsf;
            if(curr>ans){
                ans = curr;
            }
        }
        return ans;
    }
}
