#include <bits/stdc++.h>

 

int findDuplicate(vector<int> &arr, int n){

    int i=0;

    while(i<n){

          if(arr[i]!=arr[arr[i]]){

              swap(arr[i],arr[arr[i]]);

          }

          else if(arr[i]==arr[arr[i]])return arr[i];

          else i++;

 

        

    }

    return 0;

    // Write your code here.

}
